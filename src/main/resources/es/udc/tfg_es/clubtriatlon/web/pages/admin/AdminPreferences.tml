<form t:type="Form" t:id="selectLanguageForm">

	<t:errors/>
	
    <div class="field">
        <t:label for="language"/>
        <span class="entry">
        	<t:select t:id="language" model="languageList" validate="required"/>
        </span>
    </div>

    <div class="button">
		<input type="submit" value="${message:button-apply}"/>
    </div>

</form>
		
